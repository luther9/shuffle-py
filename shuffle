#!/usr/bin/env python3

# Copyright 2018 Luther Thompson

import argparse
import sys

parser = argparse.ArgumentParser(add_help=False)
parser.add_argument(
  'uniques', type=int, help='Number of unique, unshuffled cards',
)


def main(argv):
  print(parser.parse_args(argv[1:]).uniques)


if __name__ == '__main__':
  main(sys.argv)
